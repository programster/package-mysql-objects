{
    "name": "programster/mysql-objects",
    "type": "library",
    "description": "Package for rapidly developing models that represent mysql tables. Tables must have an id field.",
    "keywords": ["mysql", "mysqli","models"],
    "homepage": "https://github.com/programster/package-mysql-objects",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Page",
            "email": "sdpagent@gmail.com",
            "homepage": "https://blog.programster.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "programster/core-libs": "~1.7",
        "ramsey/uuid": "^4.1",
        "programster/mysqli-lib": "^1.0"
    },
    "require-dev": {
        "irap/autoloader": "1.0.*",
        "irap/migrations": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Programster\\MysqlObjects\\": "src/"
        }
    }
}
